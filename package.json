{
	"private": true,
	"name": "scradd",
	"version": "2.0.0-prerelease",
	"description": "Custom Discord bot for the Scratch Addons server.",
	"license": "MIT",
	"homepage": "https://discord.gg/Cs25kzs889",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/scratchaddons-community/scradd.git"
	},
	"type": "module",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon . --harmony --trace-warnings",
		"format": "prettier --write \"**\" --ignore-unknown && prettier-package-json --write",
		"start": "node .",
		"types": "tsc --project jsconfig.json"
	},
	"dependencies": {
		"@discordjs/builders": "0.12.0",
		"async-exit-hook": "2.0.1",
		"difflib": "0.2.4",
		"discord.js": "13.6.0",
		"dotenv": "16.0.1",
		"fuse.js": "6.6.2",
		"json-diff": "0.9.0",
		"node-fetch": "3.2.8",
		"papaparse": "5.3.2"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "1.1.1",
		"@types/async-exit-hook": "2.0.0",
		"@types/json-diff": "0.7.0",
		"@types/node": "18.0.6",
		"@types/papaparse": "5.3.2",
		"nodemon": "2.0.19",
		"prettier": "2.5.1",
		"prettier-package-json": "2.6.0",
		"typescript": "4.7.4"
	},
	"prettier": "@redguy12/prettier-config",
	"prettier-package-json": {
		"tabWidth": 4,
		"useTabs": true,
		"expandUsers": true
	}
}
