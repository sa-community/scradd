{
	"name": "scradd",
	"version": "4.0.1",
	"private": true,
	"homepage": "https://discord.gg/FPv957V6SD",
	"type": "module",
	"main": "./dist/index.js",
	"scripts": {
		"build": "rm -rf ./dist && tsc --project tsconfig.json",
		"dev": "tsc --project tsconfig.json --watch",
		"format": "prettier --write \"**\" \"!./dist/\" --ignore-unknown --cache",
		"lint": "eslint",
		"serve": "nodemon --env-file .env --enable-source-maps . --ext js,json",
		"start": "node --enable-source-maps .",
		"test": "node --run build && cd dist && node --enable-source-maps --test"
	},
	"browserslist": [
		"chrome 80",
		"edge 80",
		"opera 67",
		"ff 86"
	],
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"@sa-community/addons-data": "1.42.0",
		"discord.js": "14.22.1",
		"match-sorter": "8.1.0",
		"mongoose": "8.17.2",
		"posthtml-parser": "0.12.1",
		"strife.js": "4.4.0"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "3.4.4",
		"@total-typescript/ts-reset": "0.6.1",
		"@types/node": "<=22.10",
		"eslint": "9.33.0",
		"eslint-config-cobaltt7": "1.3.1",
		"nodemon": "3.1.10",
		"prettier": "3.6.2",
		"typescript": "5.9.2"
	},
	"optionalDependencies": {
		"@railway/cli": "4.6.3"
	},
	"engines": {
		"node": "^22.10.0",
		"npm": ">=9.0.0"
	}
}
